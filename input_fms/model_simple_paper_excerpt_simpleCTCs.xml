<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="AAFMFramework">
			<or name="Packages">
				<graphics key="collapsed" value="false"/>
				<feature name="python-sat"/>
				<feature name="pycosat"/>
				<feature name="pyPicosat"/>
				<feature name="pyglucose"/>
			</or>
			<or name="Solvers">
				<graphics key="collapsed" value="false"/>
				<feature name="MiniSAT"/>
				<feature name="PicoSAT"/>
				<feature name="Glucose"/>
			</or>
			<alt mandatory="true" name="System">
				<graphics key="collapsed" value="false"/>
				<feature name="Linux"/>
				<feature name="Win"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>MiniSAT</var>
				<var>python-sat</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Win</var>
				<not>
					<var>pyPicosat</var>
				</not>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>PicoSAT</var>
				<not>
					<var>python-sat</var>
				</not>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>PicoSAT</var>
				<not>
					<var>pyglucose</var>
				</not>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Glucose</var>
				<not>
					<var>pycosat</var>
				</not>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Glucose</var>
				<not>
					<var>pyPicosat</var>
				</not>
			</imp>
		</rule>
	</constraints>
</featureModel>
