<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="horizontal"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="1"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="AAFMFramework">
			<and name="Packages">
				<graphics key="collapsed" value="false"/>
				<and mandatory="true" name="DepMng">
					<graphics key="collapsed" value="false"/>
					<and mandatory="true" name="pip">
						<feature name="pipdeptree"/>
					</and>
					<feature mandatory="true" name="setuptools"/>
					<feature name="wheel"/>
					<feature name="pkgresources"/>
				</and>
				<or name="APISolvers">
					<graphics key="collapsed" value="false"/>
					<feature name="pythonsat"/>
					<feature name="pycosat"/>
					<feature name="pylgl"/>
					<feature name="pyPicosat"/>
					<feature name="pyglucose"/>
					<feature name="pycryptosat"/>
					<feature name="satyrn"/>
					<feature name="satispy"/>
					<feature name="umo"/>
					<feature name="pydepqbf"/>
				</or>
				<or name="ParserTechnology">
					<graphics key="collapsed" value="false"/>
					<feature name="antlr4python3runtime"/>
					<feature name="SimpleParse"/>
					<and name="larkparser">
						<feature name="ebnf"/>
					</and>
				</or>
				<or name="Utils">
					<graphics key="collapsed" value="false"/>
					<feature name="six"/>
					<feature name="pybind11"/>
				</or>
			</and>
			<and mandatory="true" name="Metamodels">
				<feature name="FeatureModel"/>
				<feature name="CNFModel"/>
				<feature name="DecisionModel"/>
			</and>
			<and name="AutomatedReasoning">
				<or mandatory="true" name="Solvers">
					<graphics key="collapsed" value="false"/>
					<feature name="MiniSAT"/>
					<feature name="PicoSAT"/>
					<feature name="Glucose"/>
					<feature name="CaDiCaL"/>
					<feature name="Minicard"/>
					<feature name="Maple"/>
					<feature name="CryptoMiniSat"/>
					<feature name="Lingeling"/>
					<feature name="CBC"/>
					<feature name="DepQBF"/>
				</or>
				<or name="Operations">
					<graphics key="collapsed" value="false"/>
					<feature name="ValidModel"/>
					<feature name="ValidConfiguration"/>
					<feature name="ValidProduct"/>
					<feature name="AllProducts"/>
					<feature name="DeadFeatures"/>
					<feature name="CoreFeatures"/>
					<feature name="ErrorDetection"/>
					<feature name="ErrorDiagnosis"/>
				</or>
			</and>
			<or name="Interoperability">
				<feature name="FeatureIDE"/>
				<feature name="SPLOT"/>
				<feature name="UVL"/>
			</or>
			<alt mandatory="true" name="System">
				<feature name="Linux"/>
				<feature name="Win"/>
			</alt>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>MiniSAT</var>
				<disj>
					<var>satispy</var>
					<disj>
						<var>umo</var>
						<var>pythonsat</var>
					</disj>
				</disj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Lingeling</var>
				<disj>
					<var>satispy</var>
					<disj>
						<var>pylgl</var>
						<var>pythonsat</var>
					</disj>
				</disj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>CBC</var>
				<var>umo</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>PicoSAT</var>
				<disj>
					<var>pycosat</var>
					<var>pyPicosat</var>
				</disj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Glucose</var>
				<disj>
					<var>pythonsat</var>
					<var>pyglucose</var>
				</disj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>DepQBF</var>
				<var>pydepqbf</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<disj>
					<var>CaDiCaL</var>
					<disj>
						<var>Maple</var>
						<var>Minicard</var>
					</disj>
				</disj>
				<var>pythonsat</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>CryptoMiniSat</var>
				<var>pycryptosat</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<disj>
					<var>satyrn</var>
					<var>pyglucose</var>
				</disj>
				<var>pybind11</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>pythonsat</var>
				<var>six</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>Win</var>
				<conj>
					<not>
						<var>umo</var>
					</not>
					<not>
						<var>pyPicosat</var>
					</not>
				</conj>
			</imp>
		</rule>
		<rule>
			<eq>
				<var>CNFModel</var>
				<var>AutomatedReasoning</var>
			</eq>
		</rule>
		<rule>
			<imp>
				<conj>
					<var>Linux</var>
					<var>pip</var>
				</conj>
				<var>pkgresources</var>
			</imp>
		</rule>
		<rule>
			<disj>
				<not>
					<var>UVL</var>
				</not>
				<var>ParserTechnology</var>
			</disj>
		</rule>
	</constraints>
</featureModel>
